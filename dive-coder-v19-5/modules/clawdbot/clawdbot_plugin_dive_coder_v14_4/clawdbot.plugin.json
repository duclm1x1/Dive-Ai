  "uiHints": {
    "repoRoot": { "label": "Repo root", "placeholder": "/path/to/repo" },
    "pythonPath": { "label": "PYTHONPATH", "placeholder": ".shared/vibe-coder-v13" },
    "ragKbDir": { "label": "RAG KB dir", "placeholder": ".vibe/kb" }
