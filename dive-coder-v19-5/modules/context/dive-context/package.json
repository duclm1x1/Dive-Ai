{
  "name": "dive-context",
  "version": "1.0.0",
  "description": "MCP server for fetching up-to-date documentation from GitHub - specialized for Dive Coder v14",
  "type": "module",
  "main": "dist/index-github.js",
  "bin": {
    "dive-context": "./dist/index-github.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index-github.ts",
    "start": "node dist/index-github.js",
    "update-cache": "node dist/index-github.js --update-cache",
    "clear-cache": "node dist/index-github.js --clear-cache",
    "list-libraries": "node dist/index-github.js --list-libraries"
  },
  "keywords": ["mcp", "documentation", "github", "dive-coder", "n8n", "automation"],
  "author": "Manus AI",
  "license": "MIT",
  "packageManager": "pnpm@10.28.1",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@octokit/rest": "^22.0.1",
    "commander": "^14.0.2",
    "express": "^5.2.1",
    "openai": "^6.16.0",
    "undici": "^7.19.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.10",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
